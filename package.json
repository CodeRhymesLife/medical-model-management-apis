{
  "name": "medical-model-manager-apis",
  "version": "1.0.0",
  "description": "APIs for managing medical models",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:drryanjames/medical-model-manager-apis.git"
  },
  "author": "Ryan James <rcjames1004@gmail.com>",
  "license": "none",
  "private": true,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.5.1"
  },
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "cross-env NODE_ENV=development nodemon --ignore lib --exec babel-node src",
    "prod:build": "rimraf lib && babel src --out-dir lib --copy-files --ignore .test.js",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib --name medical-model-manager-apis && pm2 logs medical-model-manager-apis",
    "prod:stop": "pm2 delete medical-model-manager-apis",
    "mongo:start": "start mongod",
    "flow": "flow",
    "lint": "esw *.js src --color",
    "lint:watch": "yarn lint -- --watch",
    "precommit": "yarn lint && yarn test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register --ui bdd --reporter spec --colors src --recursive --require babel-polyfill",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "university of washington",
    "UW",
    "REST",
    "API",
    "boilerplate",
    "medical",
    "model",
    "management"
  ],
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "debug": "^2.4.5",
    "dotenv": "^4.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "express-request-id": "^1.4.0",
    "express-session": "^1.15.6",
    "express-validation": "1.0.2",
    "express-winston": "2.5.0",
    "glob": "^7.1.2",
    "google-auth-library": "^1.6.1",
    "helmet": "3.12.0",
    "http-status": "1.0.1",
    "joi": "^13.4.0",
    "load-json-file": "^5.0.0",
    "method-override": "^2.3.10",
    "mongoose": "4.7.4",
    "morgan": "1.9.0",
    "nodemon": "^1.17.5",
    "pm2": "^2.10.4",
    "request-context": "^2.0.0",
    "rimraf": "^2.6.2",
    "validator": "^10.4.0",
    "winston": "2.4.1",
    "winston-context": "^0.0.7"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "5.1.4",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "flow-bin": "^0.74.0",
    "husky": "0.14.3",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.5.0",
    "supertest": "^3.1.0",
    "supertest-as-promised": "^4.0.2",
    "validate-commit-msg": "^2.14.0"
  }
}
