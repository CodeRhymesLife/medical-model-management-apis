{
  "name": "medical-model-manager-apis",
  "version": "1.0.0",
  "description": "APIs for managing medical models",
  "repository": {
    "type": "git",
    "url": "git@github.com:drryanjames/medical-model-manager-apis.git"
  },
  "author": "Ryan James <rcjames1004@gmail.com>",
  "license": "none",
  "private": true,
  "engines": {
    "node": ">=10.4.1",
    "npm": ">=6.1.0",
    "yarn": ">=1.7.0"
  },
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "yarn build && cross-env NODE_ENV=development nodemon build/index.js",
    "build": "yarn lint && yarn build-ts",
    "build-ts": "rimraf build && tsc && ./node_modules/.bin/copyfiles -u 1 src/config/settings.json build",
    "prod:start": "yarn build && cross-env NODE_ENV=production pm2 start build --name medical-model-manager-apis && pm2 logs medical-model-manager-apis",
    "prod:stop": "pm2 delete medical-model-manager-apis",
    "mongo:start": "sudo service mongod start",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "precommit": "yarn lint && yarn test",
    "test": "yarn build && cross-env NODE_ENV=test MONGO_HOST=mongodb://localhost/medmod-test ./node_modules/.bin/mocha build/**/*.test.js --ui bdd --reporter spec --colors src --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "university of washington",
    "UW",
    "REST",
    "API",
    "medical",
    "model",
    "management",
    "typescript"
  ],
  "dependencies": {
    "@types/glob": "^5.0.35",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "debug": "^2.4.5",
    "dotenv": "^4.0.0",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "express-request-id": "^1.4.0",
    "express-session": "^1.15.6",
    "express-validation": "1.0.2",
    "express-winston": "2.5.0",
    "glob": "^7.1.2",
    "google-auth-library": "^1.6.1",
    "helmet": "3.12.0",
    "http-status": "^1.2.0",
    "joi": "^13.4.0",
    "load-json-file": "^5.0.0",
    "method-override": "^2.3.10",
    "mongoose": "4.7.4",
    "morgan": "1.9.0",
    "nodemon": "^1.17.5",
    "pm2": "^2.10.4",
    "pretty-error": "^2.1.1",
    "reflect-metadata": "^0.1.12",
    "request-context": "^2.0.0",
    "rimraf": "^2.6.2",
    "typegoose": "^5.2.1",
    "validator": "^10.4.0",
    "winston": "2.4.1",
    "winston-context": "^0.0.7"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
    "@types/bluebird": "^3.5.21",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/compression": "^0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/express-winston": "^2.4.4",
    "@types/helmet": "^0.0.38",
    "@types/http-status": "^0.2.30",
    "@types/joi": "^13.3.0",
    "@types/load-json-file": "^2.0.7",
    "@types/method-override": "^0.0.31",
    "@types/mocha": "^5.2.4",
    "@types/mongodb": "^3.1.1",
    "@types/mongoose": "^5.2.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.5.2",
    "@types/passport": "^0.4.5",
    "@types/request": "^2.47.1",
    "@types/rimraf": "^2.0.2",
    "@types/supertest": "^2.0.4",
    "@types/supertest-as-promised": "^2.0.35",
    "@types/validator": "^9.4.1",
    "chai": "4.1.2",
    "copyfiles": "^2.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "5.1.4",
    "husky": "0.14.3",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.5.0",
    "supertest": "^3.1.0",
    "supertest-as-promised": "^4.0.2",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.4.2",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "validate-commit-msg": "^2.14.0"
  }
}
